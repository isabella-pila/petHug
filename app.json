{
  "expo": {
    "name": "PetHug",
    "slug": "pet-teste",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_adote.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo_adote.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_adote.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.isabellapila.petteste"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/Itim-Regular.ttf"
          ]
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "65e12272-1103-476a-9b4c-7c8c095a6360"
      }
    }
  }
}
